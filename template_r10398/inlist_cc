&cc_controls

  ! name of supernova model used to calculate ejected, remnant and fallback mass
   model_name = 'delayed'

  ! name of folder where core-collapse data will be saved
   cc_data_directory = 'cc_data'

  ! name of files with information of the collapsing star & binary system at core-collapse
   filename_for_star_data = 'star_at_core_collapse'
   filename_for_binary_data = 'binary_at_core_collapse'

  ! maximum mass for a neutron star in Msun (used if model_name is 'rapid' or 'delayed')
   max_ns_mass = 2.5d0

  ! add natal-kick from within MESA. NOT READY TO USE
   add_asymmetric_kick = .false.

  ! continue binary evolution after core-collapse. NOT READY TO USE
   continue_binary_evolution = .false.

/ ! end of cc_controls namelist
